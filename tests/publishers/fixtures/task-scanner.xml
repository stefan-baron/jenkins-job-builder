<?xml version="1.0" encoding="utf-8"?>
<project>
  <publishers>
    <hudson.plugins.tasks.TasksPublisher>
      <healthy>0</healthy>
      <unHealthy>1</unHealthy>
      <thresholdLimit>low</thresholdLimit>
      <pluginName>[TASKS]</pluginName>
      <defaultEncoding>UTF-8</defaultEncoding>
      <canRunOnFailed>true</canRunOnFailed>
      <useStableBuildAsReference>true</useStableBuildAsReference>
      <useDeltaValues>true</useDeltaValues>
      <thresholds>
        <unstableTotalAll>1</unstableTotalAll>
        <unstableTotalHigh>2</unstableTotalHigh>
        <unstableTotalNormal>3</unstableTotalNormal>
        <unstableTotalLow>4</unstableTotalLow>
        <unstableNewAll>1</unstableNewAll>
        <unstableNewHigh>2</unstableNewHigh>
        <unstableNewNormal>3</unstableNewNormal>
        <unstableNewLow>4</unstableNewLow>
        <failedTotalAll>5</failedTotalAll>
        <failedTotalHigh>6</failedTotalHigh>
        <failedTotalNormal>7</failedTotalNormal>
        <failedTotalLow>8</failedTotalLow>
        <failedNewAll>5</failedNewAll>
        <failedNewHigh>6</failedNewHigh>
        <failedNewNormal>7</failedNewNormal>
        <failedNewLow>8</failedNewLow>
      </thresholds>
      <shouldDetectModules>true</shouldDetectModules>
      <dontComputeNew>false</dontComputeNew>
      <doNotResolveRelativePaths>false</doNotResolveRelativePaths>
      <high>FIXME, XXX, FIX ME</high>
      <normal>TODO, TO DO</normal>
      <low>NOTE</low>
      <ignoreCase>true</ignoreCase>
      <asRegexp>true</asRegexp>
      <pattern>src/java/**</pattern>
      <excludePattern>**/target/**, **/*.png, **/*-gen/**, src/java/repositories/**</excludePattern>
    </hudson.plugins.tasks.TasksPublisher>
  </publishers>
</project>
